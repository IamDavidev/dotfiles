{
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "editor.renderWhitespace": "none",
    "editor.suggestSelection": "first",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "typescript.disableAutomaticTypeAcquisition": true,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.associations": {
        "*.js": "javascript"
    },
    "[vue]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "editor.fontFamily": "'Dank Mono','JetBrains Mono','MesloLGS NF', 'monospace', monospace",
    "markdown.preview.fontFamily": "Dank Mono",
    "settingsSync.ignoredSettings": [
        "editor.codeLensFontFamily"
    ],
    "terminal.integrated.fontFamily": "'MesloLGS NF', monospace",
    "editor.codeLensFontFamily": "'cascadia code',monospace",
    "git.confirmSync": false,
    "editor.fontLigatures": true,
    "editor.mouseWheelZoom": true,
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.env.windows": {},
    "terminal.integrated.defaultProfile.windows": "bash",
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.external.windowsExec": "C:\\\\Windows\\\\System32\\\\bash.exe",
    "github.copilot.enable": {
        "*": true,
        "plaintext": true,
        "markdown": true,
        "scminput": false,
        "yaml": false,
        "cpp": true,
        "javascript": true,
        "typescript": true,
        "typescriptreact": true,
        "javascriptreact": true,
        "go": true,
        "Log": true
    },
    "github.copilot.advanced": {},
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.singleQuote": true,
    "prettier.semi": true,
    "prettier.arrowParens": "always",
    "editor.guides.bracketPairs": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "entity.name.type",
                    // "keyword",
                    "constant",
                    "storage.modifier",
                    "emphasis",
                    "invalid",
                    "markup",
                    "entity.other.attribute-name",
                    "entity.name.method",
                    "meta.import.js",
                    "entity.name.function",
                ],
                "settings": {
                    "fontStyle": "bold"
                }
            },
            {
                "scope": [
                    "comment",
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "entity.parameter.method",
                    "support.function",
                    "entity.other.attribute-name",
                    "meta,function",
                    "constant",
                    "export",
                    "enum",
                    "entity.other.inherited-class",
                    "enity.parameter.function",
                    "entity.parameter.method",
                    "entity.name.method",
                    "meta.function.parameters",
                    "entity.name.class",
                    "entity.name.function.parameter",
                    "support.constant",
                    "entity.name.type",
                    "variable.parameter.js",
                    "entity.name.tag.css"
                ],
                "settings": {
                    "fontStyle": "italic "
                }
            },
            {
                "scope": [
                    "comment",
                    "punctuation",
                    "punctuation.definition.comment"
                ],
                "settings": {
                    "foreground": "#AAC4FF"
                }
            },
            {
                "scope": [
                    "string",
                    "punctuation.definition.string",
                    "punctuation.definition.string.begin",
                    "punctuation.definition.string.end",
                ],
                "settings": {
                    "foreground": "#DEEBFF80",
                    "fontStyle": ""
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name"
                ],
                "settings": {
                    "foreground": "#54BAB9",
                    "fontStyle": "bold"
                }
            }
        ]
    },
    "editor.inlineSuggest.enabled": true,
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "editor.linkedEditing": true,
    "workbench.editor.enablePreview": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "expand",
    "editor.renderLineHighlight": "gutter",
    "editor.occurrencesHighlight": "off",
    "editor.selectionHighlight": false,
    "vscode_custom_css.imports": [
        ""
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "eslint.run": "onSave",
    "oneDarkPro.bold": true,
    "oneDarkPro.vivid": true,
    "editor.formatOnSave": true,
    "vikings-icon-theme.folders.theme": "specific",
    "[go]": {
        "editor.defaultFormatter": "golang.go"
    },
    "glassit.alpha": 255,
    "errorLens.fontWeight": "800",
    // "errorLens.fontStyleItalic": true,
    // "errorLens."
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "[astro]": {
        "editor.defaultFormatter": "astro-build.astro-vscode"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "eslint.codeActionsOnSave.mode": "all",
    "eslint.validate": [
        "javascript"
    ],
    "editor.wordWrap": "on",
    "errorLens.fontStyle": "normal",
    "liquid.format": true,
    "window.menuBarVisibility": "toggle",
    "redhat.telemetry.enabled": true,
    "workbench.productIconTheme": "icons-carbon",
    "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
    "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
    "vscode-pets.position": "explorer",
    "[python]": {
        "editor.defaultFormatter": "ms-python.python",
        "editor.formatOnType": true
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "editor.semanticTokenColorCustomizations": {
        "[Xcode Default Dark]": {
            "rules": {
                "comment": "#ff0000",
                "comment.line.number-sign.python": "#fff"
            }
        }
    },
    "workbench.colorTheme": "One Dark Darker",
    "vscode-pets.petSize": "large",
    "audioCues.lineHasInlineSuggestion": "off",
    "editor.inlayHints.fontSize": 12,
    "editor.inlayHints.padding": true,
    "javascript.inlayHints.enumMemberValues.enabled": true,
    "javascript.inlayHints.parameterNames.enabled": "all",
    "typescript.inlayHints.parameterNames.enabled": "all",
    "javascript.inlayHints.parameterTypes.enabled": false,
    "typescript.inlayHints.variableTypes.enabled": false,
    "turboConsoleLog.logFunction": "console.info",
    "turboConsoleLog.delimiterInsideMessage": "~> ",
    "editor.stickyScroll.enabled": true,
    "eslint.debug": true,
    "eslint.format.enable": true,
    "playwright.reuseBrowser": true,
    "editor.tabSize": 2,
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "symbols.hidesExplorerArrows": false,
    "[prisma]": {
        "editor.defaultFormatter": "Prisma.prisma"
    },
    "tailwindCSS.classAttributes": [
        "class",
        "className",
        "ngClass",
        "classes",
        ".*Classes"
    ],
    "workbench.iconTheme": "symbols",
    "terminal.integrated.fontSize": 12,
    "workbench.sideBar.location": "right",
    "twind.enable": true,
    "twind.styles": [
        "style",
        "styled"
    ]
}